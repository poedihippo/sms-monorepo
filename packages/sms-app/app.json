{
  "expo": {
    "name": "MOVES",
    "slug": "melandas-app",
    "privacy": "hidden",
    "orientation": "portrait",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover"
    },
    "userInterfaceStyle": "automatic",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#313132",
      "translucent": false
    },
    "platforms": [
      "ios",
      "android"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nalphas.moves"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.nalphas.melandas",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_PHONE_STATE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
